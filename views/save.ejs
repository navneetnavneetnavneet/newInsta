<% include ./partials/header.ejs %>

    <div class="w-full min-h-screen bg-zinc-900">
        <div class="posts w-full flex flex-wrap justify-start gap-1 overflow-y-auto">
            <% user.savePosts.forEach(function(post){ %>
                <div class="post w-[32.5%] h-32 overflow-hidden">
                    <img class="w-full h-full object-cover" src="/images/uploads/<%= post.picture %>" alt="">
                </div>
            <% }) %>
        </div>
    </div>

