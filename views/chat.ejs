<% include ./partials/header.ejs %>

    <div class="w-full min-h-screen bg-zinc-600">
        <% alluser.forEach(function(user){ %>
            <a href="/chat/user/<%= user._id %>">
                <div class="w-full px-4 py-4 flex gap-x-2 items-center border-b-2">
                    <div class="w-[8vw] h-[8vw] rounded-full bg-sky-100 overflow-hidden">
                        <img class="w-full h-full object-cover" src="/images/uploads/<%= user.profileImage %>" alt="">
                    </div>
                    <h4 class="text-white text-xl font-semibold"><%= user.username %></h4>
                </div>
            </a>
        <% }) %>
    </div>

<% include ./partials/footer.ejs %>